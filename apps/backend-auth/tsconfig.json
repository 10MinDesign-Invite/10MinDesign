{
  "compilerOptions": {
    "target": "es2016",                                  
    "module": "NodeNext",                                
    "rootDir": "./src",                                  
    "outDir": "./dist",                                   
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,
    "skipLibCheck": true                                 
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "next.config.js",
    ".next/types/**/*.ts"
, "../../packages/lib/utils.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
